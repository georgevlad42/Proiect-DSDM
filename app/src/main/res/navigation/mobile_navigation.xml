<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_photo">

    <fragment
        android:id="@+id/navigation_photo"
        android:name="com.gve.proiectdsdm.ui.photo.PhotoFragment"
        android:label="@string/title_photo" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.gve.proiectdsdm.ui.search.SearchFragment"
        android:label="@string/title_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.gve.proiectdsdm.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
